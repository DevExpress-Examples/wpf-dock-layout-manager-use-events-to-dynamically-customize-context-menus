<Window x:Class="DockManager_MenuCustomization.Window1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Title="Window1" Height="462" Width="441" xmlns:dxdo="http://schemas.devexpress.com/winfx/2008/xaml/docking">
    <Grid>
        <dxdo:DockLayoutManager Margin="12" Name="dockManager1" ShowingMenu="dockManager1_ShowingMenu">
            <dxdo:LayoutGroup x:Name="rootGroup" Orientation="Horizontal">

                <dxdo:LayoutGroup x:Name="layoutGroup3" Orientation="Vertical">
                    <dxdo:LayoutPanel x:Name="paneSolutionExplorer" Caption="Solution Explorer">
                    </dxdo:LayoutPanel>
                    <dxdo:LayoutPanel x:Name="paneProperties" Caption="Properties">
                    </dxdo:LayoutPanel>

                </dxdo:LayoutGroup>
                <dxdo:DocumentGroup x:Name="documentGroup1">
                    <dxdo:DocumentPanel>
                        <RichTextBox />
                    </dxdo:DocumentPanel>
                </dxdo:DocumentGroup>

            </dxdo:LayoutGroup>
        </dxdo:DockLayoutManager>
    </Grid>
</Window>
